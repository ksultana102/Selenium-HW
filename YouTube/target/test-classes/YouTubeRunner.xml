<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="YouTube Automation" parallel="false">

    <parameter name="url" value="https://www.youtube.com//"/>
    <test name = "YouTube Home">
        <classes>
            <class name="home.NegativeTestLogin"/>
            <class name="home.TestAccount"/>
            <class name="home.TestLibrary"/>
            <class name="home.TestTrending"/>
            <class name="home.TestSearchEngine"/>
        </classes>
    </test>

</suite>